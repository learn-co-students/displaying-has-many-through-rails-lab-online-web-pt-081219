<h1><%= @patient.name %></h1>

<ul>
    <%= @patient.appointments.each do |appointment|%>
        <li><%= link_to appointment.doctor.name, doctor_path(appointment.doctor)%>
        <li><%= appointment.appointment_datetime.strftime("%A, %B %d, %Y") %><%= appointment.appointment_datetime.strftime(" at %H:%M") %></li>
    <% end %>
</ul>
